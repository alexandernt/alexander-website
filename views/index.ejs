            <div class="nav__header--text">
              <span><a class="palabra_tachada">Alexander <br> Tisko</a></span>
       
            </div>
           
          </div>

          <div class="nav__icon relative">
             <div class="box1"></div>
              <div class="box2"></div>
              <div class="box3"></div>
          </div>
 
          </div>
